// Conversão de apenas um no decimal
//Por exemplo: 86
x=input('digite o número decimal a ser convertido: ') 
str=dec2bin(x)
printf('Número inteiro convertido para binário:\n')
disp(str) 
// Conversão de vários nos decimais contidos em um vetor (e.g., 12, 45 e 135)
//Por exemplo: x=[12;45;135]
x=input('digite o vetor com os decimais a serem convertidos: ') 
z=dec2bin(x)
printf('Vários números inteiros em um vetor convertido para binário:\n')
disp(z)
